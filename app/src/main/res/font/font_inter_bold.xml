    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="o">
    <!--regular-->
    <font
        android:font="@font/inter_bold"
        android:fontStyle="normal"

        app:font="@font/inter_bold"
